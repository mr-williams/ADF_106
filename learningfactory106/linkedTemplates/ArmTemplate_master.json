{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "learningfactory106"
		},
		"API_autos_medallion_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'API_autos_medallion'"
		},
		"dataengineering106_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'dataengineering106'"
		},
		"API_autos_medallion_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://car-api2.p.rapidapi.com/api/models"
		},
		"API_autos_medallion_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "sqladminuser"
		},
		"API_car_project_properties_typeProperties_existingClusterId": {
			"type": "string",
			"defaultValue": "0624-084500-rxnpq0pa"
		},
		"dataengineering106_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://dataengineering106.dfs.core.windows.net/"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "learningfactory106_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2024-03-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"API_autos_medallion_password": {
						"value": "[parameters('API_autos_medallion_password')]"
					},
					"dataengineering106_accountKey": {
						"value": "[parameters('dataengineering106_accountKey')]"
					},
					"API_autos_medallion_properties_typeProperties_url": {
						"value": "[parameters('API_autos_medallion_properties_typeProperties_url')]"
					},
					"API_autos_medallion_properties_typeProperties_userName": {
						"value": "[parameters('API_autos_medallion_properties_typeProperties_userName')]"
					},
					"API_car_project_properties_typeProperties_existingClusterId": {
						"value": "[parameters('API_car_project_properties_typeProperties_existingClusterId')]"
					},
					"dataengineering106_properties_typeProperties_url": {
						"value": "[parameters('dataengineering106_properties_typeProperties_url')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}